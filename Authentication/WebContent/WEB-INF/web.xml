<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>Authentication</servlet-name>
		<servlet-class>com.ram.authentication.Authentication</servlet-class>

		<init-param>

			<param-name>p1</param-name>
			<param-value>oracle.jdbc.driver.OracleDriver</param-value>

		</init-param>

		<init-param>

			<param-name>p2</param-name>
			<param-value>jdbc:oracle:thin:@localhost:1521:xe,admin,admin
			</param-value>

		</init-param>
		<servlet-mapping>
			<servlet-name>Authentication</servlet-name>
			<url-pattern>/insert</url-pattern>

		</servlet-mapping>

		<welcome-file-list>
			<welcome-file>

				Authentication.html
			</welcome-file>

		</welcome-file-list>


	</servlet>


</web-app>