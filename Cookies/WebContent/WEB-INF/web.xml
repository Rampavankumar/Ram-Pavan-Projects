<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>one</servlet-name>
		<servlet-class>com.ram.cookie.CookieExample</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>two</servlet-name>
		<servlet-class>com.ram.cookie.Cookie1</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>one</servlet-name>
		<url-pattern>/cook</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>two</servlet-name>
		<url-pattern>/cer</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>cookies.html</welcome-file>
	</welcome-file-list>
</web-app>